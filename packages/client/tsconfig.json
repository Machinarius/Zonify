{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "module": "es6",
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "sourceMap": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*"],
  "typeAcquisition": {
    "enable": true,
    "include": ["./src/assets.d.ts"]
  }
}